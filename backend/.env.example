# AWS Configuration
AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY_ID
AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_ACCESS_KEY
AWS_REGION=ap-southeast-2

# S3 Configuration
S3_BUCKET=YOUR_S3_BUCKET_NAME

# DynamoDB Configuration
DYNAMODB_TABLE=OnboardingSubmissions

# Admin Credentials (change these!)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password_here

# Super Admin Credentials (has access to deleted records)
SUPER_ADMIN_USERNAME=superadmin
SUPER_ADMIN_PASSWORD=your_super_secure_password_here

# Server Configuration
PORT=5000

# CORS Configuration (comma-separated origins for production)
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.vercel.app

# Cognito Configuration
COGNITO_USER_POOL_ID=YOUR_COGNITO_USER_POOL_ID
COGNITO_APP_CLIENT_ID=YOUR_COGNITO_APP_CLIENT_ID
